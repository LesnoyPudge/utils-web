{
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig to read more about this file */
        /*
    
        */
        /* Language and Environment */
        "target": "ES2022",
        "lib": [
            "ES2023"
        ],
        "moduleDetection": "auto",
        /*
    
        */
        /* Modules */
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "baseUrl": ".",
        "paths": {
            // "@root": ["src/index.ts"]
        },
        "types": [],
        "resolveJsonModule": true,
        "noUncheckedSideEffectImports": true,
        /*
    
        */
        /* JavaScript Support */
        "allowJs": true,
        "checkJs": true,
        /*
    
        */
        /* Emit */
        "noEmit": true,
        "outDir": "build",
        "sourceMap": true,
        "declaration": true,
        /*
    
        */
        /* Interop Constraints */
        "esModuleInterop": true,
        "verbatimModuleSyntax": false,
        /*
    
        */
        /* Type Checking */
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noFallthroughCasesInSwitch": true,
        /*
    
        */
        /* Completeness */
        "skipLibCheck": true,
        /*
        
        */
        /* Compiler Diagnostics */
        "explainFiles": false,
        /*
        
        */
        /* Output Formatting */
        "pretty": true
    }
}